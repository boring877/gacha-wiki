{
  "name": "gacha-wiki",
  "version": "2.9.9",
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.0",
    "@astrojs/vercel": "^8.2.6",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.13.2",
    "chart.js": "^4.5.0",
    "gsap": "^3.13.0",
    "path-browserify": "^1.0.1",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "bun@1.2.21",
  "scripts": {
    "dev": "astro dev",
    "dev:setup": "bun run scripts/dev-setup.js && bun run dev",
    "build": "astro check && astro build",
    "build:prod": "bun run scripts/prod-setup.js && astro check && astro build && bun run scripts/compress.ts",
    "build:analyze": "astro check && astro build && bun run scripts/analyze.ts",
    "build:vercel": "bun run scripts/prod-setup.js && astro build",
    "compress": "bun run scripts/compress.ts",
    "analyze": "bun run scripts/analyze.ts",
    "perf": "bun run scripts/perf-test.ts",
    "preview": "astro preview",
    "start": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "sync": "astro sync",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "validate": "bun run scripts/validate-data.ts",
    "seo:score": "node scripts/seo-score.js",
    "seo:audit": "node scripts/seo-audit.js",
    "seo:live": "node scripts/seo-audit.js https://gacha-wiki.vercel.app",
    "seo:monitor": "node scripts/seo-monitor.js",
    "seo:vitals": "node scripts/core-web-vitals.js",
    "seo:validate": "node scripts/seo-validator.js",
    "hooks:install": "git config core.hooksPath .githooks",
    "version:patch": "node scripts/version-manager.js patch",
    "version:minor": "node scripts/version-manager.js minor",
    "version:major": "node scripts/version-manager.js major",
    "version:tag": "node scripts/version-manager.js --tag-only",
    "version:check-tags": "node scripts/version-manager.js --check-missing",
    "prepare": "husky || exit 0",
    "pre-commit": "lint-staged"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,astro}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/path-browserify": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.20.5"
  }
}
