---
import BlogDetailLayout from '../../../../layouts/busty-burst/BlogDetailLayout.astro';
import { blogConfig } from '../../../../data/busty-burst/blog.js';

export function getStaticPaths() {
  return blogConfig.posts.map(post => ({
    params: { slug: post.slug },
    props: { post },
  }));
}

const { post } = Astro.props;
---

<BlogDetailLayout post={post} />
