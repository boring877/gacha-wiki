---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { merlinData, merlinSEO } from '../../../../data/zone-nova/characters/merlin.js';

export const prerender = true;
---

<CharacterLayout
  title={merlinSEO.title}
  description={merlinSEO.description}
  gameTitle={merlinData.name}
>
  <CharacterNavigation currentCharacter={merlinData} />
  <CharacterPageTemplate characterData={merlinData} />
</CharacterLayout>
