---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { chiyaData, chiyaSEO } from '../../../../data/zone-nova/characters/chiya.js';

export const prerender = true;
---

<CharacterLayout
  title={chiyaSEO.title}
  description={chiyaSEO.description}
  gameTitle={chiyaData.name}
>
  <CharacterNavigation currentCharacter={chiyaData} />
  <CharacterPageTemplate characterData={chiyaData} />
</CharacterLayout>
