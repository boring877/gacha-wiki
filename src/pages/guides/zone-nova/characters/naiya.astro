---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { naiyaData, naiyaSEO } from '../../../../data/zone-nova/characters/naiya.js';

export const prerender = true;
---

<CharacterLayout
  title={naiyaSEO.title}
  description={naiyaSEO.description}
  gameTitle={naiyaData.name}
>
  <CharacterNavigation currentCharacter={naiyaData} />
  <CharacterPageTemplate characterData={naiyaData} />
</CharacterLayout>
