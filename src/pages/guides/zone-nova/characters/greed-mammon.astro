---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import {
  greedMammonData,
  greedMammonSEO,
} from '../../../../data/zone-nova/characters/greed-mammon.js';

export const prerender = true;
---

<CharacterLayout
  title={greedMammonSEO.title}
  description={greedMammonSEO.description}
  gameTitle={greedMammonData.name}
>
  <CharacterNavigation currentCharacter={greedMammonData} />
  <CharacterPageTemplate characterData={greedMammonData} />
</CharacterLayout>
