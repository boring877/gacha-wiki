---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { yuisData, yuisSEO } from '../../../../data/zone-nova/characters/yuis.js';

export const prerender = true;
---

<CharacterLayout title={yuisSEO.title} description={yuisSEO.description} gameTitle={yuisData.name}>
  <CharacterNavigation currentCharacter={yuisData} />
  <CharacterPageTemplate characterData={yuisData} />
</CharacterLayout>
