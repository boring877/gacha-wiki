---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { gaiaData, gaiaSEO } from '../../../../data/zone-nova/characters/gaia.js';

export const prerender = true;
---

<CharacterLayout title={gaiaSEO.title} description={gaiaSEO.description} gameTitle={gaiaData.name}>
  <CharacterNavigation currentCharacter={gaiaData} />
  <CharacterPageTemplate characterData={gaiaData} />
</CharacterLayout>
