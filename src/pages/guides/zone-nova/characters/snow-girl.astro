---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { snowGirlData, snowGirlSEO } from '../../../../data/zone-nova/characters/snow-girl.js';

export const prerender = true;
---

<CharacterLayout
  title={snowGirlSEO.title}
  description={snowGirlSEO.description}
  gameTitle={snowGirlData.name}
>
  <CharacterNavigation currentCharacter={snowGirlData} />
  <CharacterPageTemplate characterData={snowGirlData} />
</CharacterLayout>
