---
import ZoneNovaLayout1 from '../../../../layouts/ZoneNovaLayout1.astro';
import FormattedDate from '../../../../components/FormattedDate.astro';
---

<ZoneNovaLayout1 
	title="Kela Character Information - Zone Nova - GachaWiki" 
	description="Complete information for Kela, the premier single-target healer and divine support in Zone Nova"
	gameTitle="Kela Character Information"
	showBreadcrumb={true}
	breadcrumbText="Characters"
>
	<Fragment slot="head">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, viewport-fit=cover">
		<style>

			.character-page {
				background: var(--bg-secondary);
				min-height: 100vh;
				padding: 2rem 0;
			}

			.character-container {
				max-width: 1200px;
				margin: 0 auto;
				padding: 0 2rem;
			}			.character-badge {
				padding: 0.6rem 1.2rem;
				border-radius: 25px;
				font-weight: 700;
				font-size: 0.9rem;
				text-transform: uppercase;
				letter-spacing: 0.8px;
				border: 2px solid;
				box-shadow: 0 3px 10px rgba(0,0,0,0.15);
				transition: all 0.3s ease;
			}			.ssr-badge {
				background: linear-gradient(135deg, #ff6b35, #f7931e);
				color: white;
				border-color: #ff6b35;
			}

			.sr-badge {
				background: linear-gradient(135deg, #4fc3f7, #29b6f6);
				color: white;
				border-color: #4fc3f7;
			}

			.holy-badge {
				background: linear-gradient(135deg, #ffd54f, #ffeb3b);
				color: #222;
				border-color: #ffd54f;
			}

			.healer-badge {
				background: linear-gradient(135deg, #00bcd4, #26c6da);
				color: white;
				border-color: #00bcd4;
			}

			.badges-container {
				background: linear-gradient(135deg, #f8fafc 60%, #e0f7fa 100%);
				border-radius: 14px;
				box-shadow: 0 3px 15px rgba(0,0,0,0.08);
				padding: 1.2rem 1.8rem;
				border: 1px solid var(--neutral-200);
				margin-top: 1rem;
				transition: all 0.3s ease;
				position: relative;
				overflow: hidden;
			}

			.badges-container::before {
				content: '';
				position: absolute;
				top: 0;
				left: -100%;
				width: 100%;
				height: 100%;
				background: linear-gradient(90deg, transparent, rgba(255, 183, 77, 0.1), transparent);
				transition: left 0.6s ease;
			}

			.badges-container:hover::before {
				left: 100%;
			}

			.badges-container:hover {
				transform: translateY(-2px);
				box-shadow: 0 6px 20px rgba(0,0,0,0.12);
				border-color: var(--amber-glow);
			}			.skills-table-container {
				background: var(--bg-primary);
				border-radius: 12px;
				padding: 1.5rem;
				box-shadow: 0 4px 15px rgba(164, 120, 100, 0.08);
				border: 1px solid var(--neutral-200);
			}

			.skills-table {
				width: 100%;
				border-collapse: collapse;
				background: transparent;
			}

			.skills-table th {
				background: var(--amber-glow);
				color: #222;
				padding: 1rem;
				text-align: left;
				font-weight: 700;
				border-bottom: 2px solid var(--neutral-200);
			}

			.skills-table td {
				padding: 1.2rem 1rem;
				border-bottom: 1px solid var(--neutral-200);
				vertical-align: top;
			}

			.skills-table tr:hover {
				background: rgba(255, 183, 77, 0.05);
			}

			.skills-table td:first-child {
				width: 150px;
				text-align: center;
			}

			.skills-table td:nth-child(2) {
				width: 250px;
			}

			.skills-table td:last-child {
				min-width: 400px;
			}

			.skills-table ul {
				margin: 0;
				padding-left: 1.5rem;
			}

			.skills-table li {
				margin-bottom: 0.5rem;
			}

			.skills-table strong {
				color: var(--amber-glow);
			}

			.skills-table small {
				color: var(--text-secondary);
				font-size: 0.85rem;
			}			.skill-type {
				display: inline-block;
				padding: 0.4rem 0.8rem;
				border-radius: 8px;
				font-weight: 600;
				font-size: 0.85rem;
				text-align: center;
			}

			.skill-type.normal-attack {
				background: #e3f2fd;
				color: #1976d2;
				border: 1px solid #bbdefb;
			}

			.skill-type.auto-skill {
				background: #f3e5f5;
				color: #7b1fa2;
				border: 1px solid #ce93d8;
			}

			.skill-type.ultimate {
				background: #fff3e0;
				color: #f57c00;
				border: 1px solid #ffcc02;
			}

			.skill-type.passive {
				background: #e8f5e8;
				color: #388e3c;
				border: 1px solid #a5d6a7;
			}

			.skill-type.team-skill {
				background: #fce4ec;
				color: #c2185b;
				border: 1px solid #f8bbd9;
			}			.skill-name, .awakening-name {
				font-weight: 600;
				color: var(--amber-glow);
				margin-bottom: 0.5rem;
			}

			.skill-description, .awakening-description {
				color: var(--text-secondary);
				line-height: 1.6;
			}

			.stats-grid {
				display: grid;
				gap: 1rem;
			}			.stat-item {
				background: linear-gradient(135deg, var(--bg-secondary), rgba(255, 183, 77, 0.05));
				border: 1px solid var(--neutral-200);
				border-radius: 12px;
				padding: 1.5rem;
				text-align: center;
				transition: all 0.3s ease;
				display: flex;
				flex-direction: column;
				align-items: center;
			}

			.stat-item:hover {
				transform: translateY(-2px);
				box-shadow: 0 6px 20px rgba(164, 120, 100, 0.15);
				border-color: var(--amber-glow);
			}

			.stat-label {
				font-size: 0.9rem;
				color: var(--amber-glow);
				font-weight: 600;
				display: block;
				margin-bottom: 0.5rem;
			}

			.stat-value {
				font-size: 1.3rem;
				font-weight: 700;
				color: var(--text-primary);
			}

			.character-image {
				width: 300px;
				height: 300px;
				border-radius: 20px;
				object-fit: cover;
				margin: 0 auto 2rem auto;
				display: block;
				box-shadow: 0 12px 30px rgba(0, 172, 193, 0.3);
				transition: all 0.3s ease;
			}

			.character-image:hover {
				transform: scale(1.05);
				box-shadow: 0 16px 40px rgba(0, 172, 193, 0.4);
			}

			.character-title {
				font-family: 'Space Grotesk', sans-serif;
				font-size: 3rem;
				font-weight: 700;
				background: linear-gradient(135deg, var(--amber-glow), var(--teal-crystal));
				-webkit-background-clip: text;
				-webkit-text-fill-color: transparent;
				margin: 0 0 1rem 0;
				animation: titleGlow 3s ease-in-out infinite alternate;
			}

			.character-subtitle {
				color: var(--text-secondary);
				font-size: 1.2rem;
				margin: 0;
				font-weight: 500;
			}

			.content-section {
				background: var(--bg-primary);
				border-radius: 20px;
				padding: 3rem;
				margin-bottom: 2rem;
				border: 1px solid var(--neutral-200);
				box-shadow: 0 8px 25px rgba(164, 120, 100, 0.08);
				transition: all 0.4s ease;
				animation: sectionFadeIn 0.8s ease-out;
			}

			.content-section:hover {
				transform: translateY(-2px);
				box-shadow: 0 12px 35px rgba(164, 120, 100, 0.12);
				border-color: var(--teal-crystal);
			}

			.section-title {
				font-family: 'Space Grotesk', sans-serif;
				font-size: 2rem;
				font-weight: 700;
				color: var(--text-primary);
				margin: 0 0 1.5rem 0;
				padding-bottom: 0.75rem;
				border-bottom: 2px solid var(--amber-glow);
				position: relative;
			}

			.section-title::after {
				content: '';
				position: absolute;
				bottom: -2px;
				left: 0;
				width: 60px;
				height: 2px;
				background: var(--teal-crystal);
			}

			.subsection-title {
				font-family: 'Space Grotesk', sans-serif;
				font-size: 1.5rem;
				font-weight: 600;
				color: var(--text-primary);
				margin: 2rem 0 1rem 0;
				padding-left: 1rem;
				border-left: 4px solid var(--amber-glow);
			}

			.skill-title {
				font-family: 'Space Grotesk', sans-serif;
				font-size: 1.3rem;
				font-weight: 600;
				color: var(--amber-glow);
				margin: 1.5rem 0 0.8rem 0;
			}

			.info-card {
				background: linear-gradient(135deg, rgba(255, 183, 77, 0.05), rgba(0, 172, 193, 0.05));
				border: 1px solid var(--neutral-200);
				border-radius: 15px;
				padding: 2rem;
				margin: 1.5rem 0;
				transition: all 0.3s ease;
				position: relative;
				overflow: hidden;
			}

			.info-card::before {
				content: '';
				position: absolute;
				top: 0;
				left: -100%;
				width: 100%;
				height: 100%;
				background: linear-gradient(90deg, transparent, rgba(255, 183, 77, 0.1), transparent);
				transition: left 0.6s ease;
			}

			.info-card:hover::before {
				left: 100%;
			}

			.info-card:hover {
				transform: translateY(-2px);
				box-shadow: 0 8px 25px rgba(164, 120, 100, 0.15);
				border-color: var(--teal-crystal);
			}			.awakening-table-container {
				background: var(--bg-primary);
				border-radius: 12px;
				padding: 1.5rem;
				box-shadow: 0 4px 15px rgba(164, 120, 100, 0.08);
				border: 1px solid var(--neutral-200);
			}

			.awakening-table {
				width: 100%;
				border-collapse: collapse;
				background: transparent;
			}

			.awakening-table th {
				background: var(--amber-glow);
				color: #222;
				padding: 1rem;
				text-align: left;
				font-weight: 700;
				border-bottom: 2px solid var(--neutral-200);
			}

			.awakening-table td {
				padding: 1.2rem 1rem;
				border-bottom: 1px solid var(--neutral-200);
				vertical-align: top;
			}

			.awakening-table tr:hover {
				background: rgba(255, 183, 77, 0.05);
			}

			.awakening-table ul {
				margin: 0;
				padding-left: 1.2rem;
				list-style-type: disc;
			}

			.awakening-table li {
				margin-bottom: 0.3rem;
				line-height: 1.4;
			}

			@keyframes sectionFadeIn {
				0% {
					opacity: 0;
					transform: translateY(20px);
				}
				100% {
					opacity: 1;
					transform: translateY(0);
				}
			}

			@keyframes titleGlow {
				0% {
					text-shadow: 
						0 0 10px rgba(255, 183, 77, 0.3),
						0 0 20px rgba(255, 183, 77, 0.2);
				}
				100% {
					text-shadow: 
						0 0 20px rgba(255, 183, 77, 0.6),
						0 0 30px rgba(255, 183, 77, 0.4);				}
			}

			/* Memory Card mobile layout */
			@media (max-width: 768px) {
				.memory-card-layout {
					flex-direction: column !important;
					align-items: center !important;
				}
				
				.memory-card-layout img {
					margin-bottom: 1rem;
				}
			}

			@media (max-width: 768px) {
				.character-container {
					padding: 0 1rem;
				}				.content-section {
					padding: 2rem;
				}
				
				.section-title {
					font-size: 1.8rem;
				}

				.character-title {
					font-size: 2.5rem;
				}

				.character-image {
					width: 250px;
					height: 250px;
				}

				.stats-grid {
					grid-template-columns: 1fr;
				}

				/* Character Overview mobile layout */
				.content-section div[style*="display: flex; gap: 2.5rem"] {
					flex-direction: column !important;
					align-items: center !important;
				}

				/* Memory Card mobile layout */
				.content-section div[style*="display: flex; gap: 2.5rem; align-items: flex-start"] {
					flex-direction: column !important;
					align-items: center !important;
				}

				.content-section img[alt="Kela Memory Card"] {
					width: 160px !important;
					height: 240px !important;
				}
			}
		</style>
	</Fragment>

	<div class="character-page">		<div class="character-container">
			<!-- Character Header & Overview (merged) -->
			<section class="content-section">
				<h2 class="section-title" style="font-size:2.1rem; margin-bottom:2.5rem; text-align:center;">Character Overview</h2>
				<div style="display: flex; gap: 2.5rem; align-items: flex-start; flex-wrap: wrap;">
					<!-- Hero Card (Left) -->
					<div style="flex: 1.5; min-width: 340px; max-width: 520px; display: flex; flex-direction: column; align-items: center;">
						<div style="background: linear-gradient(135deg, #f8fafc 60%, #e0f7fa 100%); border-radius: 18px; box-shadow: 0 4px 18px rgba(0,0,0,0.08); padding: 2.5rem 1.5rem 1.5rem 1.5rem; border: 1.5px solid var(--amber-glow); display: flex; flex-direction: column; align-items: center; justify-content: center; min-height: 420px; width: 100%;">
							<img src="/images/characters/kela.jpg" alt="Kela" class="character-image" loading="eager" style="margin-bottom: 1.5rem; margin-top: 0; width: 340px; height: 340px; object-fit: cover; border-radius: 16px; box-shadow: 0 6px 24px rgba(0,0,0,0.10); max-width: 100%;" />
							<h1 class="character-title" style="margin: 0; font-size: 2.8rem; text-align:center; font-weight: 700; background: linear-gradient(135deg, var(--amber-glow), var(--teal-crystal)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; animation: titleGlow 3s ease-in-out infinite alternate;">Kela</h1>
						</div>
						<!-- Badges in separate box with better styling -->
						<div class="badges-container" style="margin-top: 1.8rem;">							<div style="display: flex; justify-content: center; gap: 0.7rem; flex-wrap: wrap;">
								<span class="character-badge sr-badge">SR</span>
								<span class="character-badge holy-badge">Holy</span>
								<span class="character-badge healer-badge">Healer</span>
							</div>
						</div>
					</div>
					<!-- Stats (Right) -->
					<div style="flex: 1; min-width: 260px; max-width: 400px;">
						<div style="background: linear-gradient(135deg, #fffde7 60%, #e0f2f1 100%); border-radius: 18px; box-shadow: 0 4px 18px rgba(0,0,0,0.06); padding: 2.2rem 1.5rem; border: 1px solid var(--neutral-200); width: 100%;">						<div class="stats-grid" style="margin-bottom: 1.5rem; grid-template-columns: 1fr 1fr;">
							<div class="stat-item"><div class="stat-label" style="color:#fbc02d; font-size:1.1rem;">Faction</div><div class="stat-value" style="font-size:1.25rem; font-weight:600; letter-spacing:1px; color:#222;">Otis</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#ff9800; font-size:1.1rem;">Role</div><div class="stat-value" style="font-size:1.25rem; font-weight:700; color:#00bcd4; letter-spacing:1px;">Healer</div></div>
						</div><div class="stats-grid" style="grid-template-columns: 1fr 1fr; gap: 1.1rem;">
							<div class="stat-item"><div class="stat-label" style="color:#fbc02d;">HP</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">7,650</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#ff9800;">Attack</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">837</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#fbc02d;">Defense</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">608</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#ff9800;">Energy Regen</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">0.25/s</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#fbc02d;">Crit Rate</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">0%</div></div>
							<div class="stat-item"><div class="stat-label" style="color:#ff9800;">Crit Damage</div><div class="stat-value" style="font-size:1.15rem; font-weight:600;">50%</div></div>
						</div>
						</div>
					</div>
				</div>
			</section>			<!-- Skills Section -->
			<section class="content-section">
				<h2 class="section-title">Skills</h2>

				<div class="skills-table-container">
					<table class="skills-table">
						<thead>
							<tr>
								<th style="width: 25%;">Skill Type</th>
								<th style="width: 25%;">Skill Name</th>
								<th>Description</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td><span class="skill-type normal-attack">Normal Attack</span></td>
								<td>
									<div class="skill-name">Divine Strike</div>
									<div style="font-size: 0.8rem; color: var(--text-secondary);">Level: 10+4 • CD: 2.0s</div>
								</td>
								<td>
									<div class="skill-description">
										Deals <strong>120% attack power</strong> as Holy damage to designated enemy unit
									</div>
								</td>
							</tr>
							<tr>
								<td><span class="skill-type auto-skill">Auto Skill</span></td>
								<td>
									<div class="skill-name">Sacred Healing</div>
									<div style="font-size: 0.8rem; color: var(--text-secondary);">Level: 10+4 • Auto-trigger: 5.0s</div>
								</td>
								<td>
									<div class="skill-description">
										<ul style="margin: 0; padding-left: 1rem;">
											<li>Heals the ally with the <strong>lowest health percentage</strong></li>
											<li>Healing amount: <strong>69% attack power + 335 HP</strong></li>
											<li>Intelligent targeting ensures optimal healing distribution</li>
										</ul>
									</div>
								</td>
							</tr>
							<tr>
								<td><span class="skill-type ultimate">Ultimate</span></td>
								<td>
									<div class="skill-name">Divine Intervention</div>
									<div style="font-size: 0.8rem; color: var(--text-secondary);">Level: 10+4 • CD: 1.0s • Energy: 3</div>
								</td>
								<td>
									<div class="skill-description">
										<ul style="margin: 0; padding-left: 1rem;">
											<li>Heals the ally with the <strong>lowest HP percentage</strong></li>
											<li>Healing amount: <strong>128% attack power + 682 HP</strong></li>
											<li>Scales with attack buffs, making it potent when paired with attack-boosting allies</li>
										</ul>
									</div>
								</td>
							</tr>							<tr>
								<td><span class="skill-type passive">Passive</span></td>
								<td>
									<div class="skill-name">Master of All (万事通之力)</div>
									<div style="font-size: 0.8rem; color: var(--text-secondary);">Level: 10+4</div>
								</td>
								<td>
									<div class="skill-description">
										When healing allies below <strong>50% health</strong>, healing amount increases by <strong>24%</strong>. This passive makes Kela the ultimate clutch healer for critical moments
									</div>
								</td>
							</tr>
							<tr>
								<td><span class="skill-type team-skill">Team Skill</span></td>								<td>
									<div class="skill-name">Divine Sanctuary</div>
									<div style="font-size: 0.8rem; color: var(--text-secondary);">Level: 1</div>
									<div style="font-size: 0.8rem; color: var(--amber-glow);">Requires: 2 [Otis] OR 2 [Holy] characters</div>
								</td>
								<td>
									<div class="skill-description">
										<ul style="margin: 0; padding-left: 1rem;">
											<li><strong>Effect 1:</strong> All allies' healing received increases by <strong>15%</strong></li>
											<li><strong>Effect 2:</strong> When any ally drops below 30% HP, they gain <strong>damage reduction of 10%</strong> for 8 seconds</li>
										</ul>
									</div>
								</td>
							</tr>
						</tbody>
					</table>				</div>
			</section>

			<!-- Awakening Effects -->
			<section class="content-section">
				<h2 class="section-title">Awakening Effects</h2>
						<div class="awakening-table-container">
					<table class="awakening-table">
						<thead>
							<tr>
								<th style="width: 20%;">Awakening</th>
								<th>Effects</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td><strong>Awakening 1</strong></td>
								<td>									<ul>
										<li><strong>When healing allies below 50% health:</strong> Damage they take is reduced by <strong>10%</strong> for 10 seconds</li>
										<li><em>Stacks with memory card effect for enhanced damage reduction</em></li>
									</ul>
								</td>
							</tr>
							<tr>
								<td><strong>Awakening 2</strong></td>
								<td>
									<ul>
										<li><strong>Healing amount increases by 15%</strong></li>
									</ul>
								</td>
							</tr>
							<tr>
								<td><strong>Awakening 3</strong></td>
								<td>
									<ul>
										<li><strong>[Skills] and [Normal Attack] level and level cap +4</strong></li>
									</ul>
								</td>
							</tr>
							<tr>
								<td><strong>Awakening 4</strong></td>
								<td>
									<ul>
										<li><strong>When using healing skills:</strong> Attack power increases by <strong>20%</strong> for 10 seconds</li>
										<li><em>Self-amplifying healing cycle - each heal makes the next one stronger</em></li>
