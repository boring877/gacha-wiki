---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { tefnutData, tefnutSEO } from '../../../../data/zone-nova/characters/tefnut.js';

export const prerender = true;
---

<CharacterLayout
  title={tefnutSEO.title}
  description={tefnutSEO.description}
  gameTitle={tefnutData.name}
>
  <CharacterNavigation currentCharacter={tefnutData} />
  <CharacterPageTemplate characterData={tefnutData} />
</CharacterLayout>
