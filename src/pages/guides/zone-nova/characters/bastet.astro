---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { bastetData, bastetSEO } from '../../../../data/zone-nova/characters/bastet.js';

export const prerender = true;
---

<CharacterLayout
  title={bastetSEO.title}
  description={bastetSEO.description}
  gameTitle={bastetData.name}
>
  <CharacterNavigation currentCharacter={bastetData} />
  <CharacterPageTemplate characterData={bastetData} />
</CharacterLayout>
