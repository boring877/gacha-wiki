---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { mordredData, mordredSEO } from '../../../../data/zone-nova/characters/mordred.js';

export const prerender = true;
---

<CharacterLayout
  title={mordredSEO.title}
  description={mordredSEO.description}
  gameTitle={mordredData.name}
>
  <CharacterNavigation currentCharacter={mordredData} />
  <CharacterPageTemplate characterData={mordredData} />
</CharacterLayout>
