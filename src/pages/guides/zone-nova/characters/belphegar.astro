---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { belphegarData, belphegarSEO } from '../../../../data/zone-nova/characters/belphegar.js';

export const prerender = true;
---

<CharacterLayout
  title={belphegarSEO.title}
  description={belphegarSEO.description}
  gameTitle={belphegarData.name}
>
  <CharacterNavigation currentCharacter={belphegarData} />
  <CharacterPageTemplate characterData={belphegarData} />
</CharacterLayout>
