---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { pennyData, pennySEO } from '../../../../data/zone-nova/characters/penny.js';

export const prerender = true;
---

<CharacterLayout
  title={pennySEO.title}
  description={pennySEO.description}
  gameTitle={pennyData.name}
>
  <CharacterNavigation currentCharacter={pennyData} />
  <CharacterPageTemplate characterData={pennyData} />
</CharacterLayout>
