---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { arthurData, arthurSEO } from '../../../../data/zone-nova/characters/arthur.js';

export const prerender = true;
---

<CharacterLayout
  title={arthurSEO.title}
  description={arthurSEO.description}
  gameTitle={arthurData.name}
>
  <CharacterNavigation currentCharacter={arthurData} />
  <CharacterPageTemplate characterData={arthurData} />
</CharacterLayout>
