---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { artemisData, artemisSEO } from '../../../../data/zone-nova/characters/artemis.js';

export const prerender = true;
---

<CharacterLayout
  title={artemisSEO.title}
  description={artemisSEO.description}
  gameTitle={artemisData.name}
>
  <CharacterNavigation currentCharacter={artemisData} />
  <CharacterPageTemplate characterData={artemisData} />
</CharacterLayout>
