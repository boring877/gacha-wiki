---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { anubisData, anubisSEO } from '../../../../data/zone-nova/characters/anubis.js';

export const prerender = true;
---

<CharacterLayout
  title={anubisSEO.title}
  description={anubisSEO.description}
  gameTitle={anubisData.name}
>
  <CharacterNavigation currentCharacter={anubisData} />
  <CharacterPageTemplate characterData={anubisData} />
</CharacterLayout>
