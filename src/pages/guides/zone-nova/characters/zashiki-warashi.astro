---
import CharacterIndividualLayout from '../../../../layouts/zone-nova/CharacterIndividualLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import {
  zashikiWarashiData,
  zashikiWarashiSEO,
} from '../../../../data/zone-nova/characters/zashiki-warashi.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={zashikiWarashiSEO.title}
  description={zashikiWarashiSEO.description}
  gameTitle={zashikiWarashiData.name}
>
  <CharacterNavigation currentCharacter={zashikiWarashiData} />
  <CharacterPageTemplate characterData={zashikiWarashiData} />
</CharacterIndividualLayout>
