---
import CharacterIndividualLayout from '../../../../layouts/zone-nova/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
// Navigation removed
import { lyraData, lyraSEO } from '../../../../data/zone-nova/characters/lyra.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={lyraSEO.title}
  description={lyraSEO.description}
  gameTitle={lyraData.name}
  character={lyraData}
>
  <CharacterPageTemplate characterData={lyraData} />
</CharacterIndividualLayout>
