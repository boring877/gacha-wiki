---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { heraData, heraSEO } from '../../../../data/zone-nova/characters/hera.js';

export const prerender = true;
---

<CharacterLayout title={heraSEO.title} description={heraSEO.description} gameTitle={heraData.name}>
  <CharacterNavigation currentCharacter={heraData} />
  <CharacterPageTemplate characterData={heraData} />
</CharacterLayout>
