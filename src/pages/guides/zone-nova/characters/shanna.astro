---
import CharacterIndividualLayout from '../../../../layouts/zone-nova/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
// Navigation removed
import { shannaData, shannaSEO } from '../../../../data/zone-nova/characters/shanna.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={shannaSEO.title}
  description={shannaSEO.description}
  gameTitle={shannaData.name}
  character={shannaData}
>
  <CharacterPageTemplate characterData={shannaData} />
</CharacterIndividualLayout>
