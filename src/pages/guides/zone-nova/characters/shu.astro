---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { shuData, shuSEO } from '../../../../data/zone-nova/characters/shu.js';

export const prerender = true;
---

<CharacterLayout title={shuSEO.title} description={shuSEO.description} gameTitle={shuData.name}>
  <CharacterNavigation currentCharacter={shuData} />
  <CharacterPageTemplate characterData={shuData} />
</CharacterLayout>
