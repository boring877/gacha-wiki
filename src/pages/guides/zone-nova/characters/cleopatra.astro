---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { cleopatraData, cleopatraSEO } from '../../../../data/zone-nova/characters/cleopatra.js';

export const prerender = true;
---

<CharacterLayout
  title={cleopatraSEO.title}
  description={cleopatraSEO.description}
  gameTitle={cleopatraData.name}
>
  <CharacterNavigation currentCharacter={cleopatraData} />
  <CharacterPageTemplate characterData={cleopatraData} />
</CharacterLayout>
