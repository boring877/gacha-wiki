---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { horusData, horusSEO } from '../../../../data/zone-nova/characters/horus.js';

export const prerender = true;
---

<CharacterLayout
  title={horusSEO.title}
  description={horusSEO.description}
  gameTitle={horusData.name}
>
  <CharacterNavigation currentCharacter={horusData} />
  <CharacterPageTemplate characterData={horusData} />
</CharacterLayout>
