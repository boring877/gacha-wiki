---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { guinevereData, guinevereSEO } from '../../../../data/zone-nova/characters/guinevere.js';

export const prerender = true;
---

<CharacterLayout
  title={guinevereSEO.title}
  description={guinevereSEO.description}
  gameTitle={guinevereData.name}
>
  <CharacterNavigation currentCharacter={guinevereData} />
  <CharacterPageTemplate characterData={guinevereData} />
</CharacterLayout>
