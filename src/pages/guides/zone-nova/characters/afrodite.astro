---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { afroditeData, afroditeSEO } from '../../../../data/zone-nova/characters/afrodite.js';

export const prerender = true;
---

<CharacterLayout
  title={afroditeSEO.title}
  description={afroditeSEO.description}
  gameTitle={afroditeData.name}
>
  <CharacterNavigation currentCharacter={afroditeData} />
  <CharacterPageTemplate characterData={afroditeData} />
</CharacterLayout>
