---
import CharacterLayout from '../../../../layouts/zone-nova/CharacterLayout.astro';
import CharacterNavigation from '../../../../components/zone-nova/CharacterNavigation.astro';
import CharacterPageTemplate from '../../../../templates/zone-nova/CharacterPageTemplate.astro';
import { leviathanData, leviathanSEO } from '../../../../data/zone-nova/characters/leviathan.js';

export const prerender = true;
---

<CharacterLayout
  title={leviathanSEO.title}
  description={leviathanSEO.description}
  gameTitle={leviathanData.name}
>
  <CharacterNavigation currentCharacter={leviathanData} />
  <CharacterPageTemplate characterData={leviathanData} />
</CharacterLayout>
