---
import MercenaryIndividualLayout from '../../../../layouts/horizon-walker/MercenaryIndividualLayout.astro';
import MercenaryPageTemplate from '../../../../templates/horizon-walker/MercenaryPageTemplate.astro';
import {
  exampleMercenaryData,
  exampleMercenarySEO,
  exampleMercenaryHighlighting,
} from '../../../../data/horizon-walker/mercenaries/example-mercenary.js';

export const prerender = true;
---

<MercenaryIndividualLayout
  title={exampleMercenarySEO.title}
  description={exampleMercenarySEO.description}
  gameTitle={exampleMercenaryData.name}
  mercenary={exampleMercenaryData}
>
  <MercenaryPageTemplate
    mercenaryData={{ ...exampleMercenaryData, highlightPatterns: exampleMercenaryHighlighting }}
  />
</MercenaryIndividualLayout>
