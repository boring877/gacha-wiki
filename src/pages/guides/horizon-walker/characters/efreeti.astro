---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  efretiData,
  efretiSEO,
  efretiHighlighting,
} from '../../../../data/horizon-walker/characters/efreeti.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={efretiSEO.title}
  description={efretiSEO.description}
  gameTitle={efretiData.name}
>
  <CharacterPageTemplate characterData={{ ...efretiData, highlightPatterns: efretiHighlighting }} />
</CharacterIndividualLayout>
