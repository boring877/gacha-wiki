---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  mahariData,
  mahariSEO,
  mahariHighlighting,
} from '../../../../data/horizon-walker/characters/mahari.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={mahariSEO.title}
  description={mahariSEO.description}
  gameTitle={mahariData.name}
>
  <CharacterPageTemplate characterData={{ ...mahariData, highlightPatterns: mahariHighlighting }} />
</CharacterIndividualLayout>
