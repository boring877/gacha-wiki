---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import { grideldaData, grideldaSEO, grideldaHighlighting } from '../../../../data/horizon-walker/characters/gridelda.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={grideldaSEO.title}
  description={grideldaSEO.description}
  gameTitle={grideldaData.name}
>
  <CharacterPageTemplate characterData={{...grideldaData, highlightPatterns: grideldaHighlighting}} />
</CharacterIndividualLayout>