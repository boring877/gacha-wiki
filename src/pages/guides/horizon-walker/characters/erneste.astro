---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import { ernesteData, ernesteSEO, ernesteHighlighting } from '../../../../data/horizon-walker/characters/erneste.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={ernesteSEO.title}
  description={ernesteSEO.description}
  gameTitle={ernesteData.name}
>
  <CharacterPageTemplate characterData={{...ernesteData, highlightPatterns: ernesteHighlighting}} />
</CharacterIndividualLayout>