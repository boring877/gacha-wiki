---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  nonohaData,
  nonohaSEO,
  nonohaHighlighting,
} from '../../../../data/horizon-walker/characters/nonoha.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={nonohaSEO.title}
  description={nonohaSEO.description}
  gameTitle={nonohaData.name}
  character={nonohaData}
>
  <CharacterPageTemplate characterData={{ ...nonohaData, highlightPatterns: nonohaHighlighting }} />
</CharacterIndividualLayout>
