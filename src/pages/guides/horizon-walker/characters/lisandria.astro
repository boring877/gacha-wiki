---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  lisandriaData,
  lisandriaSEO,
  lisandriaHighlighting,
} from '../../../../data/horizon-walker/characters/lisandria.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={lisandriaSEO.title}
  description={lisandriaSEO.description}
  gameTitle={lisandriaData.name}
  character={lisandriaData}
>
  <CharacterPageTemplate
    characterData={{ ...lisandriaData, highlightPatterns: lisandriaHighlighting }}
  />
</CharacterIndividualLayout>
