---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  minEunSolData,
  minEunSolSEO,
  minEunSolHighlighting,
} from '../../../../data/horizon-walker/characters/min-eun-sol.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={minEunSolSEO.title}
  description={minEunSolSEO.description}
  gameTitle={minEunSolData.name}
  character={minEunSolData}
>
  <CharacterPageTemplate
    characterData={{ ...minEunSolData, highlightPatterns: minEunSolHighlighting }}
  />
</CharacterIndividualLayout>
