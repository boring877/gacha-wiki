---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  yeonChaeYoungData,
  yeonChaeYoungSEO,
  yeonChaeYoungHighlighting,
} from '../../../../data/horizon-walker/characters/yeon-chae-young.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={yeonChaeYoungSEO.title}
  description={yeonChaeYoungSEO.description}
  gameTitle={yeonChaeYoungData.name}
>
  <CharacterPageTemplate
    characterData={{ ...yeonChaeYoungData, highlightPatterns: yeonChaeYoungHighlighting }}
  />
</CharacterIndividualLayout>
