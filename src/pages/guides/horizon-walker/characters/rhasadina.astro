---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  rhasadinaData,
  rhasadinaSEO,
  rhasadinaHighlighting,
} from '../../../../data/horizon-walker/characters/rhasadina.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={rhasadinaSEO.title}
  description={rhasadinaSEO.description}
  gameTitle={rhasadinaData.name}
  character={rhasadinaData}
>
  <CharacterPageTemplate
    characterData={{ ...rhasadinaData, highlightPatterns: rhasadinaHighlighting }}
  />
</CharacterIndividualLayout>
