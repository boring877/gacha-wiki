---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  reDeDaData,
  reDeDaSEO,
  reDeDaHighlighting,
} from '../../../../data/horizon-walker/characters/re-de-da.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={reDeDaSEO.title}
  description={reDeDaSEO.description}
  gameTitle={reDeDaData.name}
>
  <CharacterPageTemplate characterData={{ ...reDeDaData, highlightPatterns: reDeDaHighlighting }} />
</CharacterIndividualLayout>
