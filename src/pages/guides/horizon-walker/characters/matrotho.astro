---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  matrothoData,
  matrothoSEO,
  matrothoHighlighting,
} from '../../../../data/horizon-walker/characters/matrotho.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={matrothoSEO.title}
  description={matrothoSEO.description}
  gameTitle={matrothoData.name}
  character={matrothoData}
>
  <CharacterPageTemplate
    characterData={{ ...matrothoData, highlightPatterns: matrothoHighlighting }}
  />
</CharacterIndividualLayout>
