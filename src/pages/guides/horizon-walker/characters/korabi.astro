---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  korabiData,
  korabiSEO,
  korabiHighlighting,
} from '../../../../data/horizon-walker/characters/korabi.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={korabiSEO.title}
  description={korabiSEO.description}
  gameTitle={korabiData.name}
  character={korabiData}
>
  <CharacterPageTemplate characterData={{ ...korabiData, highlightPatterns: korabiHighlighting }} />
</CharacterIndividualLayout>
