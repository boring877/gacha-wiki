---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  caniskData,
  caniskSEO,
  caniskHighlighting,
} from '../../../../data/horizon-walker/characters/canisk.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={caniskSEO.title}
  description={caniskSEO.description}
  gameTitle={caniskData.name}
  character={caniskData}
>
  <CharacterPageTemplate characterData={{ ...caniskData, highlightPatterns: caniskHighlighting }} />
</CharacterIndividualLayout>
