---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  samarika,
  samarikaSEO,
  samarikaHighlighting,
} from '../../../../data/horizon-walker/characters/samarika.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={samarikaSEO.title}
  description={samarikaSEO.description}
  gameTitle={samarika.name}
  character={samarika}
>
  <CharacterPageTemplate characterData={{ ...samarika, highlightPatterns: samarikaHighlighting }} />
</CharacterIndividualLayout>
