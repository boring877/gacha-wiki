---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  juhaData,
  juhaSEO,
  juhaHighlighting,
} from '../../../../data/horizon-walker/characters/jua.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={juhaSEO.title}
  description={juhaSEO.description}
  gameTitle={juhaData.name}
>
  <CharacterPageTemplate characterData={{ ...juhaData, highlightPatterns: juhaHighlighting }} />
</CharacterIndividualLayout>
