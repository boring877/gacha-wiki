---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  bellaData,
  bellaSEO,
  bellaHighlighting,
} from '../../../../data/horizon-walker/characters/bella.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={bellaSEO.title}
  description={bellaSEO.description}
  gameTitle={bellaData.name}
>
  <CharacterPageTemplate characterData={{ ...bellaData, highlightPatterns: bellaHighlighting }} />
</CharacterIndividualLayout>
