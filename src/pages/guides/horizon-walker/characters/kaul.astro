---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  kaulData,
  kaulSEO,
  kaulHighlighting,
} from '../../../../data/horizon-walker/characters/kaul.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={kaulSEO.title}
  description={kaulSEO.description}
  gameTitle={kaulData.name}
  character={kaulData}
>
  <CharacterPageTemplate characterData={{ ...kaulData, highlightPatterns: kaulHighlighting }} />
</CharacterIndividualLayout>
