---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  oliviaData,
  oliviaSEO,
  oliviaHighlighting,
} from '../../../../data/horizon-walker/characters/olivia.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={oliviaSEO.title}
  description={oliviaSEO.description}
  gameTitle={oliviaData.name}
  character={oliviaData}
>
  <CharacterPageTemplate characterData={{ ...oliviaData, highlightPatterns: oliviaHighlighting }} />
</CharacterIndividualLayout>
