---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  gulamData,
  gulamSEO,
  gulamHighlighting,
} from '../../../../data/horizon-walker/characters/gulam.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={gulamSEO.title}
  description={gulamSEO.description}
  gameTitle={gulamData.name}
  character={gulamData}
>
  <CharacterPageTemplate characterData={{ ...gulamData, highlightPatterns: gulamHighlighting }} />
</CharacterIndividualLayout>
