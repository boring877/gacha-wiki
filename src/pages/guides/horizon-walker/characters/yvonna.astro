---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  yvonnaData,
  yvonnaSEO,
  yvonnaHighlighting,
} from '../../../../data/horizon-walker/characters/yvonna.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={yvonnaSEO.title}
  description={yvonnaSEO.description}
  gameTitle={yvonnaData.name}
  character={yvonnaData}
>
  <CharacterPageTemplate characterData={{ ...yvonnaData, highlightPatterns: yvonnaHighlighting }} />
</CharacterIndividualLayout>
