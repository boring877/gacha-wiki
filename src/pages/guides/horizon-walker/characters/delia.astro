---
import CharacterIndividualLayout from '../../../../layouts/horizon-walker/CharacterIndividualLayout.astro';
import CharacterPageTemplate from '../../../../templates/horizon-walker/CharacterPageTemplate.astro';
import {
  deliaData,
  deliaSEO,
  deliaHighlighting,
} from '../../../../data/horizon-walker/characters/delia.js';

export const prerender = true;
---

<CharacterIndividualLayout
  title={deliaSEO.title}
  description={deliaSEO.description}
  gameTitle={deliaData.name}
  character={deliaData}
>
  <CharacterPageTemplate characterData={{ ...deliaData, highlightPatterns: deliaHighlighting }} />
</CharacterIndividualLayout>
