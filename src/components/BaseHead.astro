<script>
  if ('serviceWorker' in navigator) {
    // Register immediately for faster caching
    navigator.serviceWorker
      .register('/sw.js')
      .then(registration => {
        // Force immediate update check for latest cache
        registration.update();
        console.log('ðŸš€ Extreme caching service worker registered');
      })
      .catch(registrationError => {
        console.error('Service worker registration failed:', registrationError);
      });

    // Prefetch critical pages in background
    window.addEventListener('load', () => {
      const criticalPages = [
        '/guides/zone-nova/',
        '/guides/silver-and-blood/',
        '/clock/zone-nova/',
        '/clock/silver-and-blood/',
        '/guides/zone-nova/character-comparison/',
        '/guides/silver-and-blood/character-comparison/',
      ];

      // Prefetch all critical pages for instant navigation
      criticalPages.forEach(page => {
        fetch(page, { mode: 'no-cors' }).catch(() => {});
      });
    });
  }
</script>

